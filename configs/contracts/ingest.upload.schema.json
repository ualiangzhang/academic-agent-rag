{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ingest.upload",
  "type": "object",
  "properties": {
    "filename": {"type": "string"},
    "content_b64": {"type": "string"},
    "trace_id": {"type": "string"},
    "run_id": {"type": "string"}
  },
  "required": ["filename", "content_b64"],
  "additionalProperties": false,
  "x-errors": {
    "codes": [
      {"code": "TIMEOUT", "retryable": true},
      {"code": "INVALID_ARGUMENT", "retryable": false}
    ],
    "timeout_ms": 20000,
    "rate_limit_rps": 2,
    "idempotent": false
  },
  "x-output": {
    "type": "object",
    "properties": {
      "doc_id": {"type": "string"},
      "trace_id": {"type": "string"},
      "run_id": {"type": "string"}
    },
    "required": ["doc_id"]
  }
}
